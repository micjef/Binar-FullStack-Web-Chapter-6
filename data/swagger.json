          {
            "CarAuth": []
          }
        ],
        "tags": ["Users"],
        "summary": "Change user role to admin",
        "description": "Change user role to admin by superadmin only",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "User id for changing role to admin",
