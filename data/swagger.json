  "components": {
    "securitySchemes": {
      "CarAuth": {
        "description": "Authentication for Car Management API",
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "parameters": {
      "id": {
        "name": "id",
        "in": "path",
        "required": true,
        "description": "Car id for spesific car",
        "schema": {
          "type": "string",
          "minLength": 1,
          "maxLength": 100
        }
      }
    },
    "schemas": {
      "CarSchemaReqBody": {
        "type": "object",
        "properties": {
          "plate": {
            "type": "string"
          },
          "manufacture": {
            "type": "string"
          },
          "model": {
            "type": "string"
          },
          "image": {
            "type": "string"
          },
          "rentPerDay": {
            "type": "integer"
          },
          "capacity": {
            "type": "integer",
            "default": 2
          },
          "description": {
            "type": "string"
          },
          "transmission": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "year": {
            "type": "string"
          },
          "options": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "specs": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "availableAt": {
            "type": "string"
          },
          "isDeleted": {
            "type": "boolean",
            "default": false
          },
          "createdBy": {
            "type": "string",
            "default": null
          },
          "updatedBy": {
            "type": "string",
            "default": null
          },
          "deletedBy": {
            "type": "string",
            "default": null
          }
        }
      },
      "ArrayCar": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "plate": {
              "type": "string"
            }
          }
        }
      }
    }
  }
